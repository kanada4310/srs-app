    "react-dom": "18.2.0",
    "react-i18next": "14.1.0",
    "react-router-dom": "7.8.2",
    "react-timer-hook": "3.0.7",
    "uuid": "9.0.1"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "dev": "vite --host 0.0.0.0 --port 5174",
    "serve": "vite preview",
    "lint": "biome check",
    "lint:type": "tsc --noEmit",
    "extract": "i18next --config i18next-parser.config.mjs 'src/**/*.{ts,tsx}'",
    "format": "biome check --write",
    "tauri": "tauri",
    "add-domain": "echo https://skola.cards> dist/CNAME",
    "predeploy": "npm run add-domain && npm run build",
    "deploy": "gh-pages -d dist",
    "build-and-deploy": "npm run predeploy && npm run deploy",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "pre-commit-checks": "pnpm lint-staged && pnpm lint:type"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome check --write --unsafe --no-errors-on-unmatched",
      "biome check --no-errors-on-unmatched"
    ],
    "*.json": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@netlify/blobs": "^10.0.2",
    "@netlify/functions": "^4.1.8",
    "@tauri-apps/cli": "^2.2.2",
    "@tiptap/core": "^2.23.0",
    "@tiptap/extension-hard-break": "^2.23.0",
    "@tiptap/pm": "^2.23.0",
    "@types/lodash": "4.17.0",
    "@types/node": "22.5.5",
    "@types/react": "18.2.65",
    "@types/react-dom": "18.2.21",
    "@types/scheduler": "^0.26.0",
    "@types/uuid": "9.0.8",
    "@vitejs/plugin-react": "4.2.1",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "i18next-parser": "9.0.2",
    "lint-staged": "^16.1.2",
    "postcss": "8.4.47",
    "postcss-cli": "11.0.0",
    "postcss-preset-mantine": "1.13.0",
    "postcss-simple-vars": "7.0.1",
    "scheduler": "^0.26.0",
    "typescript": "5.5.2",
    "vite": "5.2.13",
    "vite-plugin-checker": "0.6.4",
    "vite-plugin-pwa": "^1.0.0",
    "vite-tsconfig-paths": "4.3.2"
  },
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a"
}
